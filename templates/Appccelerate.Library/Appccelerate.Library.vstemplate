<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>Appccelerate.Library</Name>
    <Description>Default template for Appccelerate libraries</Description>
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType>
    </ProjectSubType>
    <SortOrder>1000</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>Appccelerate.Library</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <Icon>__TemplateIcon.ico</Icon>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="Appccelerate.Library.csproj" File="Appccelerate.Library.csproj" ReplaceParameters="true">
      <Folder Name="Properties" TargetFolderName="Properties">
        <ProjectItem ReplaceParameters="true" TargetFileName="AssemblyInfo.cs">AssemblyInfo.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="VersionInfo.g.cs">VersionInfo.g.cs</ProjectItem>
      </Folder>
	  <ProjectItem ReplaceParameters="true" TargetFileName="Version.txt">Version.txt</ProjectItem>
	  <ProjectItem ReplaceParameters="true" TargetFileName="Appccelerate.TODO.nuspec">Appccelerate.TODO.nuspec</ProjectItem>
	  <ProjectItem ReplaceParameters="true" TargetFileName="Settings.stylecop">Settings.stylecop</ProjectItem>
    </Project>
  </TemplateContent>
</VSTemplate>